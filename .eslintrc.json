{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["@pwrs"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": ["warn", { "allowHigherOrderFunctions": true }],
    "@typescript-eslint/no-empty-interface": 0,
    "no-loops/no-loops": 0
  }
}
